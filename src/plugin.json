{
  "$schema": "https://raw.githubusercontent.com/grafana/grafana/master/docs/sources/developers/plugins/plugin.schema.json",
  "name": "PNP",
  "id": "sni-pnp-datasource",
  "type": "datasource",

  "includes": [{ "type": "dashboard", "name": "PNP Internals", "path": "dashboards/pnp_internals.json" }],

  "metrics": true,
  "annotations": false,

  "info": {
    "description": "pnp4nagios datasource",
    "author": {
      "name": "Sven Nierlein",
      "email": "sven.nierlein@consol.de",
      "url": "http://labs.consol.de"
    },
    "logos": {
      "small": "img/logo.svg",
      "large": "img/logo.svg"
    },
    "links": [
      { "name": "GitHub", "url": "https://github.com/sni/grafana-pnp-datasource" },
      { "name": "MIT License", "url": "https://github.com/sni/grafana-pnp-datasource/blob/master/LICENSE" },
      { "name": "sponsorship", "url": "https://github.com/sponsors/sni" }
    ],
    "keywords": ["naemon", "pnp4nagios", "nagios", "thruk"],
    "screenshots": [{ "name": "pnp-datasource query editor", "path": "img/screenshot.png" }],
    "version": "%VERSION%",
    "updated": "%TODAY%"
  },

  "dependencies": {
    "grafanaDependency": ">=11.0.0",
    "plugins": []
